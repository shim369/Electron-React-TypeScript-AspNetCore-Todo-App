{
  "extends": "@electron-toolkit/tsconfig/tsconfig.web.json",
  "include": [
    "src/renderer/env.d.ts",
    "src/renderer/**/*",
    "src/types/*"
, "src/renderer/components", "src/renderer/router", "src/renderer/store", "src/renderer/App.tsx", "src/renderer/utils", "src/renderer/main.tsx"  ],
  "compilerOptions": {
    "composite": true,
    "jsx": "react-jsx",
    "baseUrl": "./src",
    "paths": {
      "@renderer/*": [
        "renderer/*"
      ],
      "@components/*": [
        "renderer/components/*"
      ],
      "@pages/*": [
        "renderer/pages/*"
      ],
      "@store/*": [
        "renderer/store/*"
      ],
      "@utils/*": [
        "renderer/utils/*"
      ]
    }
  }
}
